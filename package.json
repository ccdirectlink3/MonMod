{
    "name": "monmod",
    "version": "1.2.0",
    "scripts": {
        "format": "npm run json-beautify && npm run js-beautify",
        "js-beautify": "npm run beautify-cmd -q \"**/*.js\"",
        "json-beautify": "npm run beautify-cmd \"**/*.json\"",
        "beautify-cmd": "js-beautify -js -r -f"
    },
    "dependencies": {
        "js-beautify": "^1.10.3",
        "minify-json": "^1.0.0"
    }
}